<Application x:Class="SDBR_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SDBR_WPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

            <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Generic/Generic.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Generic/ControlsStyle/Controls.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Generic/BaseColor/Dark.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Generic/AccentColor/Orange.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
            </ResourceDictionary>

        </Application.Resources>
    </Application>

